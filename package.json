{
  "name": "headstart",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest --config=jest.config.js --detectOpenHandles --forceExit --watch",
    "lint": "eslint ./src",
    "watch": "npm-run-all --parallel dev:**",
    "build:server": "webpack --config ./src/webpack/server.config.js --mode development",
    "build": "webpack --config ./src/webpack/prod.config.js --mode production",
    "dev:ui": "webpack-dev-server --config ./src/webpack/dev.config.js --mode development",
    "docker:up": "docker-compose -f ./docker/dev/docker-compose.yml up",
    "docker:down": "docker-compose -f ./docker/dev/docker-compose.yml down",
    "dev:db": "npm run docker:up",
    "dev:server": "nodemon ./src/server/index.js",
    "run:server": "nodemon ./build/server/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "migrate:up": "knex migrate:latest",
    "migrate:back": "knex migrate:rollback",
    "fixtures": "node ./src/utils/fixtures",
    "seed:run": "knex seed:run"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "apollo-server-hapi": "^2.4.8",
    "bcrypt": "^3.0.6",
    "blipp": "^3.1.3",
    "bookshelf": "^0.14.2",
    "good": "^8.1.2",
    "good-console": "^8.0.0",
    "graphql": "^14.2.1",
    "hapi": "^18.1.0",
    "hapi-cors-headers": "^1.0.3",
    "hapi-router": "^4.0.0",
    "inert": "^5.1.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.5",
    "mini.css": "^3.0.1",
    "pg": "^7.10.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.4",
    "react-dom": "^16.8.6",
    "react-form-elements": "^2.3.1",
    "react-jss": "^8.6.1",
    "styled-components": "^4.2.0",
    "the-platform": "^0.10.1",
    "vision": "^5.4.4",
    "xtconf": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@kev_nz/eslint-config": "^3.2.1",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-links": "^5.0.10",
    "@storybook/addons": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.8.4",
    "react-testing-library": "^6.1.2",
    "style-loader": "^0.23.1",
    "typesetter": "^2.0.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
